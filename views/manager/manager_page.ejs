<!doctype html>
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <title>1号店UIManager-<%=moduleConfig.moduleName %></title>
    <link rel="stylesheet" href="/manager/fonts/yhd-icon/style.css">
    <link rel="stylesheet" href="/manager/stylesheets/manager.css">
</head>
<body manager-module-name="<%=pageName %>">
    <header>
        <h1>1UI</h1>
        <ul>
            <li class="icon-download"><i>下载模块</i></li>
            <li class="icon-mobile"><i>二维码</i></li>
            <li class="icon-browser showSource"><i>查看源码</i></li>
            <li class="icon-camera"><i>快照</i></li>
            <li class="icon-database"><i>页面数据</i></li>
            <li class="icon-info2"><i>显示信息</i></li>
            <li><a href="/pages/preview/<%=projectName %>/<%=pageName %>" class="icon-earth" target="_origin_view"><i>打开链接</i></a></li>
        </ul>
    </header>

    <div class="manager-content">
        <div class="manager-page-info">
            <div class="manager-info-wrapper">
                <div><label>页面：</label><span class="manager-page-attr-name"><%=pageName||"未知"%></span></div>
                <div><label>开发者：</label><span class="manager-page-attr-developper"></span><%=moduleConfig.developper||"未知"%></div>
                <div><label>邮箱：</label><span class="manager-page-attr-email"></span><%=moduleConfig.email||"未知"%></div>
                <label>配置：</label>
                <pre><%=moduleDataToString%></pre>
            </div>
        </div>
        <div class="manager-page-show">
            <!-- <a id="watch-source">查看源代码</a> -->
            <!-- 引入html文件 -->
            <div id="iframe-wrapper">
                <iframe id="page-manager-iframe" name="page-manager-iframe" scrolling="yes" src="/pages/preview/<%=projectName %>/<%=pageName%>" frameborder="0" style="height:100%;overflow:auto;"></iframe>
            </div>
        </div>
        <div class="manager-page-analyse">
            <% for(i=0;i<htmls.length;i++){%>
                <div class="module-ui-wrapper">
                    <%- htmls[i]%>
                    <div class="manager-page-tip">
                        <div class="manager-page-tip-arrow"></div>
                        <span><%=moduleData[modules[i]].uiInfo%></span>
                    </div>
                    <a class="module-ui-download" download href="/components/download/<%=projectName %>/<%=modules[i]%>">点击下载模块</a>
                </div>  
            <%}%>
        </div>


        <!-- 查看源码 -->
        <div class="source-code-wrapper">
            <pre><%=pageSource%></pre>
            <div class="close icon-cross3"></div>
        </div>

        <!-- 二维码 -->
<!--         <div class="source-code-wrapper">
            <pre><%=pageSource%></pre>
            <div class="close"></div>
            <img src="http://chart.apis.google.com/chart?cht=qr&chl=http://localhost/pages/preview/<%=projectName %>/<%=pageName %>&chs=200x200" alt="">
        </div> -->



    </div>

    <footer>&copy; TvvT 2014.</footer>
    <script src="/manager/scripts/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="/manager/scripts/manager_page.js"></script>
</body>
</html>