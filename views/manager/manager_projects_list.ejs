<!doctype html>
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <title>Zero-所有项目</title>
    <link rel="stylesheet" href="/manager/fonts/yhd-icon/style.css">
    <link rel="stylesheet" href="/manager/stylesheets/base.css">
    <link rel="stylesheet" href="/manager/stylesheets/manager_projects_list.css">
    <script type="text/javascript">
    var rendDate={
        link:'<%=link%>'
    };
    
    if(location.hostname==='localhost'){
        window.location.href=rendDate.link;
    }

    </script>
    <script src="/manager/scripts/jquery-2.1.1.min.js"></script>
</head>
<body>
    
    <header>
        <a href="/"><h1>Zero</h1><sub>by TvvT</sub></a>
        <ul id="nav">
            <!--<li class="icon-collect"><i>我的收藏</i></li>-->
            <!-- <li class="icon-download"><i>下载模块</i></li> -->
            <!-- <li class="icon-mobile"><i>二维码</i></li> -->
            <!-- <li class="icon-browser showSource"><i>查看源码</i></li> -->
        </ul>
        <div class="input-append">
            <input class="input-mini" type="search" name="search" value="0"> 
            <span class="add-on">search</span>
        </div>
    </header>

    <div class="manager_box">
        <div class="manager_projects_list">
            <% projects.forEach(function(project,index){%>
                <div class="project-wrapper">
                    <div class="folder">
                    <img src="http://d7.yihaodianimg.com/N00/M0B/43/C3/CgQCtlLXpRWABmYgAAKfGFRXweU24300_450x450.jpg" data-url="<%=link%>/<%=project%>/pages" >
                    </div>
                    <div class="listp">
                    <span id="<%=project%>" class="collect" title="收藏" >
                    </span>
                    <span>
                    <%=project%>
                    </span>
                    <span>
                        最新修改时间：2014-08-08
                    </span>
                    <a href="http://www.baidu.com" target="_blank" >交互地址(请点这里)</a>
                    <a href="javascript:;" target="_parent">视觉地址(请点这里)</a>
                    </div>
                </div>
            <%})%>
        </div>
        <div class="iframeContent">
            <i>X</i>
            <iframe src="" ></iframe>
        </div>
    </div>
    
    <div class="fixedLayer">
    </div>

    <footer>&copy; TvvT 2014.</footer>
    <script type="text/javascript" src="/manager/scripts/manager_projects_list.js"></script>
    

</body>
</html>